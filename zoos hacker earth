#include <iostream>
#include<string.h>
using namespace std;
int main() {
	string s;
	cin>>s;
	int n= s.length();
	int i,cz=0,co=0;
	for(i=0;i<n;i++){
	    if(s[i]=='z'){
	        cz=cz+1;
	    }
	    else if(s[i]=='o'){
	        co=co+1;
	    }
	}
	if(co==(2*cz)){
		cout<<"Yes"<<endl;
	}
	else{
		cout<<"No"<<endl;
	}
}
