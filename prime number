#include<iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int flag;
    if(n>3){
    for(int i=2;i<n-1;i++){
        if(n%i==0){
            flag=1;
            break;
        }
        else{
            flag=0;
            continue;
        }
    }
    }
    else  if(n==2 ||n==3){
        flag=0;
    }
    if(flag==0){
        cout<<" prime number"<<endl;
    }
    else{
        cout<<"not prime number"<<endl;
    }
}
